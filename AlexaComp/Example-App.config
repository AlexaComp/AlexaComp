<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  <log4net>
    <appender name="LogFileAppender" type="log4net.Appender.FileAppender">
      <param name="File" value="AlexaCompLOG.log" />
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
      <appendToFile value="false" />
      <rollingStyle value="Size" />
      <maxSizeRollBackups value="2" />
      <maximumFileSize value="10MB" />
      <staticLogFileName value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="%date [%thread] %-5p %c - %message%newline" />
      </layout>
    </appender>
    <appender name="SensorListAppender" type="log4net.Appender.FileAppender">
      <file value="SensorList.txt" />
      <appendToFile value="false" />
      <rollingStyle value="Size" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%message%newline" />
      </layout>
    </appender>
    <root>
      <level value="ALL" />
      <appender-ref ref="LogFileAppender" />
    </root>
    <logger additivity="false" name="SensorListAppender">
      <level value="Debug"/>
      <appender-ref ref="SensorListAppender" />
    </logger>
  </log4net>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
  <appSettings>
    <add key="HOST" value="null" />
    <add key="mqttUSER" value="null" />
    <add key="mqttPASS" value="null" />
    <add key="mqttSERVER" value="null" />
    <add key="mqttPORT" value="null" />
    <add key="AUTH" value="null" />
    <add key="PORT" value="null" />
  </appSettings>
</configuration>